<launch>
    <node pkg="hmi_bridge" exec="hmi_bridge" name="hmi_bridge" output="screen">
        <remap from="~/roll"        to="/pose/roll"/>
        <remap from="~/pitch"       to="/pose/pitch"/>
        <remap from="~/heading"     to="/pose/heading"/>
        <remap from="~/latitude"    to="/pose/latitude"/>
        <remap from="~/longitude"   to="/pose/longitude"/>
        <remap from="~/altitude"    to="/pose/altitude"/>
    </node>
</launch>